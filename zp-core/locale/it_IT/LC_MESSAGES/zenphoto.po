msgstr "<em>E</em>"

#: zp-core/admin-options.php:1607
msgid "Default search"
msgstr "Ricerca predefinita"

#: zp-core/admin-options.php:1610
msgid "<em>New</em>"
msgstr "<em>Nuova</em>"

#: zp-core/admin-options.php:1610
msgid "<em>Within</em>"
msgstr "<em>All'interno</em>"

