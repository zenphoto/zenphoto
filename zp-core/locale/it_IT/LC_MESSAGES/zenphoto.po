msgid "Treat spaces as"
msgstr "Tratta spazi come"

#: zp-core/admin-options.php:1602
msgid "<em>space</em>"
msgstr "<em>spazio</em>"

#: zp-core/admin-options.php:1602
msgid "<em>OR</em>"
msgstr "<em>O</em>"

#: zp-core/admin-options.php:1602
